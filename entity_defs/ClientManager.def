        </SetTurnFlag>
        <UnitAttack>
            <Exposed/> 
            <Arg> ARRAY <of> UINT16 </of> </Arg>
            <Arg> ARRAY <of> UINT16 </of> </Arg>
            <Arg> BOOL </Arg>
        </UnitAttack>

